{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "api/**/*": {
      "runtime": "nodejs22.x"
    }
  },
  "git": {
    "deploymentEnabled": {
      "production": true,
      "preview": true
    }
  },
  "ignoreCommand": "echo ok"
}
